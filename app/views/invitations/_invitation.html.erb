<div id="<%= dom_id invitation %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= invitation.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Token:</strong>
    <%= invitation.token %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Accepted at:</strong>
    <%= invitation.accepted_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Membership:</strong>
    <%= invitation.membership_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Team:</strong>
    <%= invitation.team_id %>
  </div>
</div>
